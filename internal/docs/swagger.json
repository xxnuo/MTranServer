                    {
                        "ApiKeyQuery": []
                    }
                ],
                "description": "返回所有支持的翻译语言代码",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "翻译"
                ],
                "summary": "获取支持的语言列表",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "500": {
