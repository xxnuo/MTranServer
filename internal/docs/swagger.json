                "parameters": [
                    {
                        "type": "string",
                        "description": "API Token",
                        "name": "token",
                        "in": "query"
                    },
                    {
                        "description": "沉浸式翻译请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/main.ImmeTranslateRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.ImmeTranslateResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
